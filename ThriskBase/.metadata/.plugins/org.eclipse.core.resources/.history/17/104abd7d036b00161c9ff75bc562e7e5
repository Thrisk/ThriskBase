package com.thrisk.base.items;

import com.thrisk.base.Main;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class CustomItemMaterial extends Item {
	public CustomItemMaterial( String IName, String myTexture){
	    this.setUnlocalizedName(IName);
		this.setTextureName(Main.MODID + ":" + myTexture);
		this.setCreativeTab(CreativeTabs.tabMaterials);
	}
}
